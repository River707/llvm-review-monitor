{
    "name": "LLVM Review Monitor",
    "manifest_version": 2,
    "icons": {
        "136": "img/icon.png"
    },
    "permissions": [
        "alarms",
        "notifications",
        "storage",
        "tabs",
        "http://reviews.llvm.org/*",
        "https://reviews.llvm.org/*"
    ],
    "background": {
        "persistent": false,
        "scripts": [
            "browser-polyfill.min.js",
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "version": "0.0"
}